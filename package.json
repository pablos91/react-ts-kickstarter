{
  "name": "react-app",
  "version": "1.0.0",
  "description": "",
  "main": "electron.js",
  "scripts": {
    "analyze": "source-map-explorer --only-mapped 'dist/*.js'",
    "start": "webpack serve --open",
    "build": "webpack --node-env production",
    "stage": "webpack --node-env production --env target=stage",
    "test": "jest --watch",
    "electron:build": "webpack --node-env production --env electron=true",
    "electron:serve": "electron . --serve --remote-debugging-port=9222",
    "electron:win": "npm run electron:build && electron-builder build --windows --x64 --ia32",
    "electron:mac": "npm run electron:build && electron-builder build --mac",
    "electron:mac64": "npm run electron:build && arch -x86_64 electron-builder build --mac"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "dotenv-webpack": "^7.0.3",
    "env-cmd": "^10.1.0",
    "i18next": "^13.1.5",
    "i18next-browser-languagedetector": "^2.2.4",
    "i18next-locize-backend": "^1.7.1",
    "i18next-xhr-backend": "1.5.1",
    "next": "^12.1.0",
    "normalize.css": "^8.0.1",
    "react": "^16.14",
    "react-dom": "^16.14",
    "react-hook-form": "^7.28.0",
    "react-hot-loader": "^4.12.7",
    "react-i18next": "^11.15.6",
    "react-router-dom": "^6.3.0",
    "react-use-validator": "^1.0.4",
    "regenerator-runtime": "0.13.3",
    "stacktrace-js": "^2.0.2",
    "validator": "^13.7.0",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@hot-loader/react-dom": "^16.14.0",
    "@jest/types": "^28.1.0",
    "@testing-library/react": "^13.2.0",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^11.15.44",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "autoprefixer": "^10",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.3",
    "core-js": "^3.21.1",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^6.7.1",
    "electron": "^17",
    "electron-builder": "^23.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "source-map-explorer": "^2.5.2",
